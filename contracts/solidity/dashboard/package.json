{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bignumber.js": "9.0.0",
    "bootstrap": "^3.3.7",
    "chart.js": "^2.8.0",
    "less": "^3.9.0",
    "less-plugin-clean-css": "^1.5.1",
    "less-watch-compiler": "^1.10.0",
    "moment": "^2.20.1",
    "npm-run-all": "^4.1.2",
    "react": "^16.6.3",
    "react-bootstrap": "^0.31.5",
    "react-chartjs-2": "^2.6.4",
    "react-dom": "^16.6.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^2.1.1",
    "react-transition-group": "^4.3.0",
    "truffle": "^5.0.10",
    "web3": "^1.2.4"
  },
  "scripts": {
    "build-css": "lessc --clean-css src/css/app.less src/app.css",
    "watch-css": "npm run build-css && less-watch-compiler src/css src app.less",
    "start-js": "react-scripts start",
    "setup": "./scripts/copy-contracts.sh ../build/contracts",
    "start": "npm run setup && npm-run-all -p watch-css start-js",
    "build": "npm run build-css && sh -ac '. .env.${REACT_APP_ENV}; react-scripts build'",
    "build:keep-dev": "REACT_APP_ENV=keep-dev npm run build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "js:lint": "eslint src/**/*.js src/**/*.jsx",
    "js:lint:fix": "eslint --fix src/**/*.js src/**/*.jsx"
  },
  "devDependencies": {
    "eslint-config-keep": "git+https://github.com/keep-network/eslint-config-keep.git#0.2.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
