{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bignumber": "^1.1.0",
    "bootstrap": "^3.3.7",
    "chart.js": "^2.7.1",
    "less": "^2.7.3",
    "less-plugin-clean-css": "^1.5.1",
    "less-watch-compiler": "^1.10.0",
    "moment": "^2.20.1",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-bootstrap": "^0.31.5",
    "react-chartjs-2": "^2.6.4",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "require-symlink": "^0.2.0",
    "truffle": "^4.0.3",
    "truffle-contract": "^3.0.1",
    "web3": "^1.0.0-beta.26"
  },
  "scripts": {
    "build-css": "lessc --clean-css src/css/app.less src/app.css",
    "watch-css": "npm run build-css && less-watch-compiler src/css src app.less",
    "start-js": "require-link && react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "require-link && react-scripts build",
    "test": "require-link && react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "requireSymlinks": {
    "contracts": "../build/contracts"
  }
}
